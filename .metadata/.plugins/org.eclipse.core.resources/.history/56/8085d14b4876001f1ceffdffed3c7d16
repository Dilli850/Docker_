package com.wipro.runner;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.CommandLineRunner;
import org.springframework.stereotype.Component;

import com.wipro.entity.Employee;
import com.wipro.service.IEmployeeServiceMgmt;
@Component
public class EmployeeRunner implements CommandLineRunner {
@Autowired
private IEmployeeServiceMgmt employeeServiceMgmt;
	@Override
	public void run(String... args) throws Exception {
		
		// TODO Auto-generated method stub
		Employee employee=new Employee();
		employee.setEmpName("rakesh");
		employee.setEmpDept("security");
		employee.setEmpSal(53525.256);
		String msg=employeeServiceMgmt.EmployeeRegister(employee);
		System.out.println(msg);

	}

}
